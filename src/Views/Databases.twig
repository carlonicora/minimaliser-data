<?php
namespace {{ data.attributes.namespace }}Data\{{ data.attributes.objectNamePlural }}\Databases;

use CarloNicora\Minimalism\Services\MySQL\Data\SqlField;
use CarloNicora\Minimalism\Services\MySQL\Data\SqlTable;
use CarloNicora\Minimalism\Services\MySQL\Enums\FieldOption;
use CarloNicora\Minimalism\Services\MySQL\Enums\FieldType;

#[SqlTable(name: '{{ data.id }}', databaseIdentifier: '{{ data.attributes.databaseIdentifier }}')]
enum {{ data.attributes.objectNamePlural }}Table
{
{% for fieldIdentifier in data.relationships['fields'].data %}{% set field = lookupIncluded('field', fieldIdentifier.id, included) %}
{% set option = false%}
{% if field.attributes.name == 'createdAt' %}
{% set option = 'TimeCreate'%}
{% elseif field.attributes.name == 'updatedAt' %}
{% set option = 'TimeUpdate'%}
{% elseif field.meta.option %}
{% set option = field.meta.option %}
{% endif %}
    #[SqlField(fieldType: FieldType::{{ field.attributes.type }}{% if option != false %} ,fieldOption: FieldOption::{{ option }}{% endif %})]
    case {{ field.attributes.name }};
{% endfor %}
}